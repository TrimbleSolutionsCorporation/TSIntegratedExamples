    <!-- ########## -->
    <!-- Other Packages -->
    <!-- ########## -->
    <PackageReference Update="Fusion"                                       Version="3.5.264" />
    <PackageReference Update="Newtonsoft.Json"                              Version="12.0.1" />
    <PackageReference Update="Microsoft.Xaml.Behaviors.Wpf"                 Version="1.1.39" />
    <PackageReference Update="Microsoft.CSharp"                             Version="4.7.0" />
    <PackageReference Update="Microsoft.Office.Interop.Excel"               Version="15.0.4795.1000" />
  </ItemGroup>

</Project>