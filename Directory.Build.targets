<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- ExcludeAssets="runtime" ensures reference dlls are not copied to bin output folders, Private=False
       PrivateAssets=all ensures the nuget package created will not depend on these nugets -->

  <ItemGroup>
    <!-- ########## -->
    <!-- Tekla Structures API and Related Version Specific Packages -->
    <!-- ########## -->
    <PackageReference Update="Tekla.Application.Library"                    Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures"                             Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Catalogs"                    Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Datatype"                    Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Dialog"                      Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Drawing"                     Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Model"                       Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Plugins"                     Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Service"                     Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.BIM.DirectManipulation"                 Version="2022.0.10075"  ExcludeAssets="runtime" />
    <PackageReference Update="Tekla.Structures.Plugins.DirectManipulation"  Version="2022.0.10075"  ExcludeAssets="runtime" />

    <!-- ########## -->
    <!-- TS Related NON-Version Specific Packages -->
