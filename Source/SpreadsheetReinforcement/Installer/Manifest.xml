<?xml version="1.0" encoding="utf-8"?>
<TEP Version="2.0">

    <Product Id="SpreadsheetReinforcement" UpgradeCode="F4B705B9-9BC8-4CF3-A41E-EA78569A85C9" Version="1.15" Language="1033"
		Name="Spreadsheet Reinforcement" Manufacturer="Trimble Solutions, Inc."
		Description= "This extension reads in spreadsheet design data and applies reinforcement component footings and piles in the model to match."
		IconPath="%TEPDEFINITIONFILEFOLDER%\Icon.png">
		<TeklaVersions>
			<TeklaVersion Name="2020.0"/>
			<MinTeklaVersion Name="2017.0"/>
			<MaxTeklaVersion Name="2099.1"/>		
		</TeklaVersions>
	</Product>
	 
	<SourcePathVariables>
		<SourcePathVariable Id="ExtensionName" Value="SpreadsheetReinforcement"/>
		<SourcePathVariable Id="TepOutputFolder" Value="%TEPDEFINITIONFILEFOLDER%\output" Comment="This provides the location where the package builder is to save the package."/>
		<SourcePathVariable Id="SourceCatalog" Value="%TEPDEFINITIONFILEFOLDER%\Catalog"/>
		<SourcePathVariable Id="SourceImages" Value="%TEPDEFINITIONFILEFOLDER%\ProgramData\Extensions\Images"/>
		<SourcePathVariable Id="SourceMessages" Value="%TEPDEFINITIONFILEFOLDER%\ProgramFiles\Messages\DotAppsStrings"/>
		<SourcePathVariable Id="SourceBin" Value="%TEPDEFINITIONFILEFOLDER%\ProgramFiles\Nt\bin"/>
		<SourcePathVariable Id="SourceBitmaps" Value="%TEPDEFINITIONFILEFOLDER%\ProgramData\Bitmaps"/>
		<SourcePathVariable Id="SourceDrawingPlugins" Value="%SourceBin%\plugins\Tekla\Drawings\%ExtensionName%"/>
		<SourcePathVariable Id="SourceModelPlugins" Value="%SourceBin%\plugins\Tekla\Model\%ExtensionName%"/>
		<SourcePathVariable Id="SourceDrawingApps" Value="%SourceBin%\applications\Tekla\Drawings\%ExtensionName%"/>
		<SourcePathVariable Id="SourceModelApps" Value="%SourceBin%\applications\Tekla\Model\%ExtensionName%"/>
		<SourcePathVariable Id="SourceCommmon" Value="%TEPDEFINITIONFILEFOLDER%\ProgramData\Environments\common"/>
		<SourcePathVariable Id="SourceUsaRoot" Value="%TEPDEFINITIONFILEFOLDER%\ProgramData\Environments\USA"/>
    </SourcePathVariables>
	
	<TargetPathVariables>
		<PathVariable Id="ExtensionName" Value="SpreadsheetReinforcement"/>
	    <PathVariable Id="ExtensionsRootDir" Value="%commonEnvFolder%\extensions"/>
		<PathVariable Id="ModelPluginsDir" Value="%ExtensionsRootDir%\plugins\tekla\model\%ExtensionName%"/>
		<PathVariable Id="DrawingsPluginsDir" Value="%ExtensionsRootDir%\plugins\tekla\drawings\%ExtensionName%"/>
		<PathVariable Id="ModelAppsDir" Value="%ExtensionsRootDir%\applications\tekla\model\%ExtensionName%"/>
		<PathVariable Id="DrawingAppsDir" Value="%ExtensionsRootDir%\applications\tekla\drawings\%ExtensionName%"/>		
		<PathVariable Id="BitmapsDir" Value="%ENVDIR%\..\bitmaps"/>
		<PathVariable Id="ExtensionsDir" Value="%ENVDIR%\..\Extensions"/>	
		<PathVariable Id="TargetUsaRoot" Value="%ENVDIR%\USA"/>
		<PathVariable Id="TargetUsimp" Value="%ENVDIR%\usimp"/>
		<PathVariable Id="TargetUsmet" Value="%ENVDIR%\usmet"/>
		<PathVariable Id="TargetCommonDir" Value="%commonEnvFolder%"/>
	</TargetPathVariables>

	<Component Id="TheExtensionComponent" Guid="E3C646FA-12CA-49C9-8BDF-3D9258CCE46A">
		<!-- <File Id="CatalogBitmaps" Source="%SourceBitmaps%\*.*" Target="%BitmapsDir%" Recursive="yes" /> -->
		<File Id="RibbonImages" Source="%SourceImages%\*.*" Target="%ExtensionsDir%\Images" Recursive="yes" />
		<File Id="ApplicationFiles" Source="%SourceModelApps%\*.*" Target="%ModelAppsDir%" Recursive="yes" />
		<!-- <File Id="TranslationsFiles" Source="%SourceMessages%\*.*" Target="%ModelAppsDir%" Recursive="yes" /> -->
		<File Id="GlobalCommon" Source="%SourceCommmon%\*.*" Target="%TargetCommonDir%" Recursive="yes" />
		<File Id="GlobalComCopyImp" Source="%SourceCommmon%\*.*" Target="%TargetUsimp%" Recursive="yes" />
		<File Id="GlobalComCopyMet" Source="%SourceCommmon%\*.*" Target="%TargetUsmet%" Recursive="yes" />
		<!-- <File Id="USARootFiles" Source="%SourceUsaRoot%\*.*" Target="%TargetUsaRoot%" Recursive="yes" /> -->
		<File Id="ComponentCatalogFiles" Source="%SourceCatalog%\*.*" Target="%ModelAppsDir%" Recursive="yes" />
	</Component>

	<Feature Id="TheExtensionFeature" Title="ExtensionFeature">
		<ComponentRef ReferenceId="TheExtensionComponent" />
	</Feature>

 </TEP>